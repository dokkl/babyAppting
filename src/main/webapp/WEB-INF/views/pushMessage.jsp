                        <h1 class="page-header">
                            푸시 메시지 보내기
                        </h1>
                        <ol class="breadcrumb">
                            <li>
                                <i class="fa fa-dashboard"></i>  <a href="/">Dashboard</a>
                            </li>
                            <li class="active">
                                <i class="fa fa-table"></i> push message send
                            </li>
                        </ol>
                    </div>
                </div>
                <!-- /.row -->

                <form action="/pushMessage" method="post">
                    <input type="text" name="msg" size="40" required />
                    <input type="submit" value="보내기" />
                    <input type="reset" value="초기화" />

                <div class="row">
                    <div class="col-lg-12">
                        <h2>전체회원</h2>
                        <div class="table-responsive">
                            <table class="table table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th><input type="checkbox" class="check" id="checkAll"></th>
                                        <th>이름</th>
                                        <th>이메일</th>
                                        <th>지역</th>
                                        <th>생년월일 (나이)</th>
                                        <th>취미</th>
                                        <th>혈액형</th>
                                        <th>종교</th>
                                        <th>image1</th>
                                    </tr>
                                </thead>
                                <tbody>
                                <c:forEach items="${memberList}" var="memberDto">
                                    <tr>
                                        <td><input type="checkbox" class="check" name="checkedList" value="${memberDto.mail}" /></td>
                                        <td>${memberDto.name}</td>
                                        <td>${memberDto.mail}</td>
                                        <td>${memberDto.address1}/${memberDto.address2}</td>
                                        <td>${memberDto.birthday} (${memberDto.age})</td>
                                        <td>${memberDto.hobby}</td>
                                        <td>${memberDto.bloodType}</td>
                                        <td>${memberDto.religion}</td>
                                        <td><img src="/image/${memberDto.image1}" alt="이미지없음1" height="84" width="84">
                                            <img src="/image/${memberDto.image2}" alt="이미지없음2" height="84" width="84"></td>
                                    </tr>
                                </c:forEach>
                                </tbody>
                            </table>
